</div>

<div class="site-section bg-light pt-0">
  <div class="container">
    <div class="row">
      <%if(products){%>
        <%products.forEach(product=>{%>
          <div class="col-sm-6 col-lg-4 text-center item mb-4 item-v2">
            <!-- <span class="onsale">Sale</span> -->
            <a href="/shop-single/<%=product.slug%>/<%=product._id%>"> <img src="/uploads/<%=product.avatar%>"
                alt="Image"></a>
            <h3 class="text-dark"><a href="/shop-single/<%=product.slug%>/<%=product._id%>"><%=product.title%></a></h3>
            <p class="price"><del>&#8377;<%=product.dummyPrice%></del> &mdash; &#8377;<%=product.price%></p>
          </div>
        <%})%>
      <%}else{%>
        <h1 style="text-align: center;">No Product</h1>
      <%}%>
      
    </div>
